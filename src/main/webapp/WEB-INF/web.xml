<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
    xmlns="http://java.sun.com/xml/ns/j2ee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>entry</servlet-name>
        <servlet-class>com.sogou.upd.ufo.lion.EntryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>entry</servlet-name>
        <url-pattern>*.ut</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>manage</servlet-name>
        <servlet-class>com.sogou.upd.ufo.lion.ManageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>manage</servlet-name>
        <url-pattern>/index.m</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manage</servlet-name>
        <url-pattern>/static.m</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manage</servlet-name>
        <url-pattern>/help.m</url-pattern>
    </servlet-mapping>
</web-app>